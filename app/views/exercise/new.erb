<h1>Add a new exercise</h1>
<form method="POST" action="/exercises ">
    <p><label for="name">Name:</label>
    <input type="text" name="name"></p>
    <p><label for="muscle_group">Muscle Group:</label>
    <input type="text" name="muscle_group"></p>
    <p><label for="length_of_time">Length of Time:</label>
    <input type="text" name="length_of_time"></p>
    <p><label for="weight">Weight:</label>
    <input type="number" name="weight"></p>
    <p><label for="sets">Sets:</label>
    <input type="number" name="sets"></p>
    <p><label for="reps">Reps:</label>
    <input type="number" name="reps"></p>
    <p><label for="notes">Notes:</label>
    <input type="text" name="notes"></p>
    <input type="submit">
</form>